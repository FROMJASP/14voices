{
  "name": "14voices",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:performance": "PERFORMANCE_TRACING=true next dev",
    "dev:analyze": "ANALYZE=true next build",
    "bundle:analyze": "ANALYZE=true next build && next start",
    "generate:types": "graphql-codegen --config codegen.yml",
    "validate:types": "tsc --noEmit -p tsconfig.typecheck.json",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "fromjasp": "bun scripts/fromjasp.ts",
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:with-migrate": "payload migrate && next build",
    "build:vercel": "node scripts/validate-test-dependencies.js && node scripts/generate-importmap.js && npm install sharp --force && next build",
    "start": "next start",
    "lint": "next lint",
    "payload": "payload",
    "seed": "tsx src/seed/run-seed.ts",
    "seed:old": "tsx src/seed/index.ts",
    "seed:alt": "tsx src/seed/index-nextjs.ts",
    "seed:voiceovers": "bun scripts/seed-voiceovers.ts",
    "admin:reset": "bun src/scripts/reset-admin-access.ts",
    "admin:check": "bun scripts/check-admin-users.ts",
    "postinstall": "node scripts/postinstall.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit -p tsconfig.typecheck.json",
    "validate:deps": "node scripts/validate-dependencies.js",
    "validate:architecture": "node scripts/validate-architecture.js",
    "validate": "bun run lint && bun run format:check && bun run typecheck && bun run test",
    "validate:build": "node scripts/validate-test-dependencies.js && bun run typecheck && bun run build",
    "validate:full": "bun run validate && bun run validate:deps && bun run validate:architecture && bun run validate:build",
    "validate:pre-push": "node scripts/validate-build-pre-push.js",
    "ci:validate": "bun run lint && bun run format:check && bun run test && bun run test:e2e"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.50.0",
    "@payloadcms/email-resend": "3.50.0",
    "@payloadcms/graphql": "3.50.0",
    "@payloadcms/live-preview-react": "3.50.0",
    "@payloadcms/next": "3.50.0",
    "@payloadcms/richtext-lexical": "3.50.0",
    "@payloadcms/storage-vercel-blob": "3.50.0",
    "@payloadcms/translations": "3.50.0",
    "@payloadcms/ui": "3.50.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4",
    "@types/pg": "^8.15.5",
    "@sentry/nextjs": "^9.34.0",
    "@types/sanitize-html": "^2.16.0",
    "@uploadthing/react": "^7.3.2",
    "@vercel/postgres": "^0.10.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dompurify": "^3.2.6",
    "dotenv": "^17.0.0",
    "dotenv-cli": "^8.0.0",
    "framer-motion": "^11.18.2",
    "gsap": "^3.13.0",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.26.0",
    "lru-cache": "^11.1.0",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.0",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "payload": "3.50.0",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redis": "^5.5.6",
    "resend": "^4.6.0",
    "sanitize-html": "^2.17.0",
    "sharp": "^0.34.3",
    "shepherd.js": "^14.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "uploadthing": "^7.7.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.53.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.18",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.6",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "lightningcss": "^1.30.1"
  }
}
